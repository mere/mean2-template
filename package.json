{
  "name": "MEAN2",
  "version": "1.0.0",
  "description": "Sample MEAN2 Application",
  "author": "Dave McManus",
  "scripts": {
    "start": "node ./server/bin/www",
    "launch:dev": "concurrently \"npm run tsc:w\" \"node ./server/bin/www\" \"gulp launch\"",
    "build": "gulp build && npm run tsc",
    "build:prod": "gulp build --production && npm run tsc && gulp del-spec-files",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "postinstall": "typings install",
    "unit-test": "karma start ./tests/unit/karma.conf.js",
    "pree2e-test": "webdriver-manager update",
    "e2e-test": "protractor ./tests/e2e/protractor.conf.js"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/core": "2.0.0-rc.5",
    "@angular/forms": "0.3.0",
    "@angular/http": "2.0.0-rc.5",
    "@angular/platform-browser": "2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "2.0.0-rc.5",
    "@angular/router": "3.0.0-rc.1",
    "body-parser": "~1.15.1",
    "bootstrap": "^3.3.6",
    "browserify": "^13.0.1",
    "cookie-parser": "~1.4.3",
    "core-js": "^2.4.0",
    "cors": "^2.7.1",
    "debug": "~2.2.0",
    "ejs": "~2.4.1",
    "es5-shim": "^4.5.8",
    "es6-shim": "^0.35.0",
    "express": "~4.13.4",
    "jquery": "^3.0.0",
    "mongodb": "^2.1.18",
    "mongoose": "^4.4.14",
    "morgan": "~1.7.0",
    "nodemailer": "^2.4.2",
    "reflect-metadata": "^0.1.3",
    "request": "^2.72.0",
    "rxjs": "5.0.0-beta.6",
    "serve-favicon": "~2.3.0",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "browser-sync": "^2.13.0",
    "concurrently": "^2.0.0",
    "del": "^2.2.1",
    "gulp": "^3.9.1",
    "gulp-cache": "^0.4.5",
    "gulp-concat": "^2.6.0",
    "gulp-cssnano": "^2.1.2",
    "gulp-if": "^2.0.1",
    "gulp-if-else": "^1.0.3",
    "gulp-ignore": "^2.0.1",
    "gulp-imagemin": "^3.0.1",
    "gulp-merge": "^0.1.1",
    "gulp-minify": "0.0.12",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-sass": "^2.3.2",
    "gulp-util": "^3.0.7",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "^0.13.22",
    "karma-chrome-launcher": "0.2.3",
    "karma-html-detailed-reporter": "^1.1.8",
    "karma-jasmine": "0.3.8",
    "karma-phantomjs-launcher": "^1.0.1",
    "lite-server": "^2.2.0",
    "phantomjs": "^2.1.7",
